{
	"manifest_version": 2,
	"name": "Scratch Account Manager",
	"description": "Easily switch accounts and view message counts of your alts!",
	"content_scripts": [
		{
			"matches":[  
				"https://scratch.mit.edu/*"
			],
			"css":[
				"styles.css"
			],
			"js":[  
				"code.js"
			]
		}
	],
	"version": "1.0",
	"background": {
		"scripts": ["fetchMessages.js"]
	},
	"permissions": [
		"https://scratch.mit.edu/*",
		"cookies",
		"storage"
	],
	"icons": {
		"128": "logo.png"
	}
}
